<!DOCTYPE html>
<html>
<head>
  <title>Cute or Boot</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
  <%= render "layouts/notifications" %>

  <%= yield %>

  <% if current_user %>
    <%= button_to "logout", session_url, method: :delete %>
  <% end %>
</body>
</html>
