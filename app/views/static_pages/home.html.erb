<script type="text/javascript">
  var loginGuest = function (event) {
    event.preventDefault();
    var currentTime = new Date();
    var minuteMark = currentTime.getMinutes();

    if (minuteMark < 15) {
      $.ajax({
        url: "/session",
        type: "POST",
        data: { session: { username: "boots", password: "password" } },
        success: function () {
          window.location.href = "/";
        }
      });
    } else if (minuteMark < 30) {
      $.ajax({
        url: "/session",
        type: "POST",
        data: { session: { username: "scruffy", password: "password" } },
        success: function () {
          window.location.href = "/";
        }
      });
    } else if (minuteMark < 45) {
      $.ajax({
        url: "/session",
        type: "POST",
        data: { session: { username: "fry", password: "password" } },
        success: function () {
          window.location.href = "/";
        }
      });
    } else {
      $.ajax({
        url: "/session",
        type: "POST",
        data: { session: { username: "whiskers", password: "password" } },
        success: function () {
          window.location.href = "/";
        }
      });
    }
  };

  $("form").on("submit", loginGuest);
</script>
