<div class="up-next-users col-xs-5 col-xs-offset-7">
  <h4>UP NEXT</h4>

  <% users.forEach(function (user) { %>
    <div class="next-user col-xs-3">
      <% if (user.pictures().length === 0) { %>
        <img
          class="img-circle"
          width="50"
          height="50"
          src="http://res.cloudinary.com/dfvpir1ro/image/upload/v1435053227/z9qumtqsirytzq8pp9pf.jpg">
      <% } else {%>
        <img
          class="img-circle"
          width="50"
          height="50"
          src="<%= user.pictures().first().escape("thumb_url") %>">
      <% } %>
    </div>
  <% }) %>
</div>
