<% var currentUser = parseInt(CURRENT_USER_ID) === parseInt(user.id) %>
<% var currentUsersVote = user.escape("current_users_vote") %>
<% var username = user.escape("username") %>
<% username = username.charAt(0).toUpperCase() + username.substring(1) %>

<% if (username) { %>
  <% if (!currentUser) { %>
    <% if (currentUsersVote === "0") { %>
      <span class="glyphicon glyphicon-remove"></span>
    <% } else if (currentUsersVote === "1") { %>
      <span class="glyphicon glyphicon-heart"></span>
    <% } else { %>
      <ul class="nav navbar-nav">
        <li>
          <button
            class="btn btn-info btn-circle btn-xl cuteit"
            data-toggle="tooltip"
            data-placement="bottom"
            title="cute!">
            <i class="glyphicon glyphicon-ok"></i>
          </button>
        </li>
        <li>
          <button
            class="btn btn-danger btn-circle btn-xl bootit"
            data-toggle="tooltip"
            data-placement="bottom"
            title="booooot">
            <i class="glyphicon glyphicon-remove"></i>
          </button>
        </li>
      </ul>
    <% } %>
  <% } %>

  <div class="navbar-header">
    <a class="navbar-brand" href="#cuties/<%= user.id %>">
      <%= username + ", " + user.age %>
    </a>
  </div>

  <% if (currentUser) { %>
    <div class="navbar-header">
      <a class="navbar-brand">I'm cute. Believe it.</a>
    </div>
  <% } %>

  <ul class="nav navbar-nav navbar-right">
    <% if (currentUser) { %>
      <li>
        <a class="add-image" href="#addImage">
          <span class="glyphicon glyphicon-picture"></span>Add Cute Picture
        </a>
      </li>
      <li>
        <a class="editprofile" href="#editprofile">
          <span class="glyphicon glyphicon-cog"></span>Edit profile
        </a>
      </li>
    <% } %>

    <li>
      <a class="toggleprofile" href="javascript:void(0)">
        <span class="glyphicon glyphicon-user"></span>Profile
      </a>
    </li>
  </ul>
<% } else { %>

<% } %>
